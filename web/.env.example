# Kaspa Network Configuration
# Options: mainnet, testnet-10
VITE_DEFAULT_NETWORK=testnet-10

# Priority Fee Configuration
# Priority fee in sompi (1 KAS = 100,000,000 sompi)
# This fee is added when priority mode is enabled for faster transaction processing
# Default: 1000 sompi = 0.00001 KAS
VITE_PRIORITY_FEE=1000

# Debug Settings
VITE_DEBUG=true

# Auto-connect on startup
VITE_AUTO_CONNECT=true

# UI Refresh Intervals (in milliseconds)
VITE_AUTO_REFRESH_INTERVAL=30000
VITE_HEARTBEAT_INTERVAL=300000

# Connection Timeouts (in milliseconds)
VITE_CONNECTION_TIMEOUT=15000
VITE_REQUEST_TIMEOUT=10000
VITE_HEALTH_CHECK_INTERVAL=30000
