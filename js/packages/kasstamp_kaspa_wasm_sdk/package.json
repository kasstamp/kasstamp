{
  "name": "@kasstamp/kaspa_wasm_sdk",
  "version": "0.0.10",
  "description": "Pure Kaspa WASM bindings from rusty-kaspa - Low-level WASM access only",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./kaspa.js": {
      "types": "./src/kaspa.d.ts",
      "default": "./src/kaspa.js"
    },
    "./kaspa.d.ts": "./src/kaspa.d.ts",
    "./wasm": {
      "types": "./src/kaspa_bg.wasm.d.ts",
      "default": "./src/kaspa_bg.wasm"
    }
  },
  "files": [
    "src"
  ],
  "sideEffects": false,
  "license": "ISC",
  "scripts": {
    "build": "npm run build:bundle",
    "build:bundle": "tsup --dts src/index.ts --format esm --splitting false --clean --no-splitting --sourcemap"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0"
  }
}
