{
  "name": "@kasstamp/stamping",
  "version": "0.0.7",
  "description": "File stamping functionality for Kasstamp",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "sideEffects": false,
  "license": "ISC",
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:bundle": "tsup src/index.ts --format esm --splitting false --clean --no-splitting --sourcemap",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly"
  },
  "dependencies": {
    "@kasstamp/chunking": "*",
    "@kasstamp/crypto": "*",
    "@kasstamp/kaspa_api": "*",
    "@kasstamp/kaspa_wasm_sdk": "*",
    "@kasstamp/tx": "*",
    "@kasstamp/utils": "*",
    "@kasstamp/wallet": "*"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
